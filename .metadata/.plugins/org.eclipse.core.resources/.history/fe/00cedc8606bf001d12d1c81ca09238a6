public class Threaddd {

	public static void main(String[] args) 
	{
		task1 t1=new task1();
		t1.start();
		System.out.println("Task2");
		for(int i=11;i<=20;i++)
		{
			System.out.print(i+" ");
		}
		
		
		System.out.println("");
		System.out.println("Task3");
		for(int i=21;i<=30;i++)
		{
			System.out.print(i+" ");
		}

	}

}
